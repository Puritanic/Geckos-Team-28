{
	"env": {
		"browser": true,
		"amd": true,
		"es6": true,
		"node": true,
		"mocha": true
	},
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"extends": ["eslint:recommended", "plugin:react/recommended"],
	"plugins": ["react"],
	"rules": {
		"no-console": 1
	}
}
